<Param>
  <Mesh>
    <Filename2>unit_square_tri_quad.inp</Filename2>
    <Filename3>unit_cube_tetras_3d.inp</Filename3>
    <RefLevel>2</RefLevel>
    <FeDegree>1</FeDegree>
  </Mesh>
  <LinearAlgebra>
    <NameMatrix>CoupledMatrix</NameMatrix>
    <NameVector>CoupledVector</NameVector>
    <Platform>CPU</Platform>
    <Implementation>Naive</Implementation>
    <MatrixFormat>CSR</MatrixFormat>
  </LinearAlgebra>
  <LinearSolver>
    <Name>CG</Name>
    <SizeBasis>50</SizeBasis>
    <Method>NoPreconditioning</Method>
    <MaxIterations>1000</MaxIterations>
    <AbsTolerance>1.0e-17</AbsTolerance>
    <RelTolerance>1.0e-17</RelTolerance>
    <DivTolerance>1.0e6</DivTolerance>
  </LinearSolver>
  <Test>
    <NumPts>1000</NumPts>
    <WithCommunication>1</WithCommunication>
  </Test>
</Param>
