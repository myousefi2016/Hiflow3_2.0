<Param>
  <OutputPrefix>fsi_couette</OutputPrefix>
  <OutputType>infofileX</OutputType>
  <Mesh>
    <Filename>fsi_couette_r2.inp</Filename>
    <RefinementLevel>0</RefinementLevel>
  </Mesh>
  <Application>
    <Type>Couette</Type>
    <Density>1000.0</Density>
    <Viscosity>1.0e-3</Viscosity>
    <Poisson>0.4</Poisson>
    <Young>5.6e5</Young>
    <LinearElast>1</LinearElast>
    <InflowHeight>0.41</InflowHeight>
    <Radii>0.2 0.35 0.5</Radii>
    <Speed>1</Speed>
    <MeshDiffFac>1.0e-6</MeshDiffFac>
    <EvaluationPoint>0 0.35</EvaluationPoint>
  </Application>
  <Instationary>
    <Endtime>50</Endtime>
    <Timestep>0.1</Timestep>
    <Theta>1</Theta>
    <SmoothStartup>1</SmoothStartup>
    <VisuIntervall>10</VisuIntervall>
  </Instationary>
  <Domain>
    <FluidMaterial>8</FluidMaterial>
    <SolidMaterial>7</SolidMaterial>
  </Domain>
  <Boundary>
    <InflowMaterial>4</InflowMaterial>
    <OutflowMaterial>0</OutflowMaterial>
    <InterfaceMaterial>5</InterfaceMaterial>
    <ObstacleMaterial>0</ObstacleMaterial>	
    <SideMaterial>6</SideMaterial>
  </Boundary>
  <LinearAlgebra>
    <NameMatrix>CoupledMatrix</NameMatrix>
    <NameVector>CoupledVector</NameVector>
    <Platform>CPU</Platform>
    <Implementation>Naive</Implementation>
    <MatrixFormat>CSR</MatrixFormat>
  </LinearAlgebra>
  <LinearSolver>
    <Name>GMRES</Name>
    <Method>RightPreconditioning</Method>
    <MaxIterations>1000</MaxIterations>
    <AbsTolerance>5.e-14</AbsTolerance>
    <RelTolerance>1.e-10</RelTolerance>
    <DivTolerance>1.e3</DivTolerance>
    <SizeBasis>500</SizeBasis>
    <Preconditioner>ilupp</Preconditioner>
    <ILUPP>
      <PreprocessingType>0</PreprocessingType>
      <PreconditionerNumber>11</PreconditionerNumber>
      <MaxMultilevels>20</MaxMultilevels>
      <MemFactor>0.8</MemFactor>
      <PivotThreshold>2.75</PivotThreshold>
      <MinPivot>0.01</MinPivot>
    </ILUPP>
  </LinearSolver>
  <NonlinearSolver>
    <Name>Newton</Name>
    <MaxIterations>10</MaxIterations>
    <AbsTolerance>1.e-13</AbsTolerance>
    <RelTolerance>1.e-13</RelTolerance>
    <DivTolerance>1.e3</DivTolerance>
    <ForcingStrategy>EisenstatWalker1xy</ForcingStrategy>
    <InitialValueForcingTerm>0.5</InitialValueForcingTerm>
    <MaxValueForcingTerm>0.9</MaxValueForcingTerm>
    <GammaParameterEW2>1.0</GammaParameterEW2>
    <AlphaParameterEW2>1.0</AlphaParameterEW2>
    <DampingStrategy>Armijoxy</DampingStrategy>
    <ThetaInitial>1.</ThetaInitial>
    <ThetaMinimal>1.e-4</ThetaMinimal>
    <ArmijoDecrease>0.5</ArmijoDecrease>
    <SufficientDecrease>1.e-4</SufficientDecrease>
    <MaxArmijoIteration>10</MaxArmijoIteration>
  </NonlinearSolver>
</Param>
