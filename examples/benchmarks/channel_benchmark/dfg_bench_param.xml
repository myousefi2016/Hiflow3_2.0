<Param>
  <OutputPrefix>ChannelBenchmarkDFG</OutputPrefix>
  <Mesh>
    <Filename>dfg_bench3d_cyl.inp</Filename>
    <InitialRefLevel>0</InitialRefLevel>
  </Mesh>
  <LinearAlgebra>
    <Platform>CPU</Platform>
    <Implementation>Naive</Implementation>
    <MatrixFormat>CSR</MatrixFormat>
  </LinearAlgebra>
  <FlowModel>
    <Density>1.0</Density>
    <Viscosity>1.0e-3</Viscosity>
    <InflowSpeed>2.25</InflowSpeed>
    <InflowHeight>0.41</InflowHeight>
    <InflowWidth>0.41</InflowWidth>
  </FlowModel>
  <DFGbenchmark>1</DFGbenchmark>
  <QuadratureOrder>6</QuadratureOrder>
  <FiniteElements>
    <VelocityDegree>2</VelocityDegree>
    <PressureDegree>1</PressureDegree>
  </FiniteElements>
  <Instationary>
    <SolveInstationary>1</SolveInstationary>
    <Method>CrankNicolson</Method>
    <Timestep>0.02</Timestep>
    <Endtime>4.0</Endtime>
  </Instationary>
  <Boundary>
    <InflowMaterial>10</InflowMaterial>
    <OutflowMaterial>12</OutflowMaterial>
    <CylinderMaterial>13</CylinderMaterial>
  </Boundary>
  <NonlinearSolver>
    <MaximumIterations>20</MaximumIterations>
    <AbsoluteTolerance>1.e-8</AbsoluteTolerance>
    <RelativeTolerance>1.e-15</RelativeTolerance>
    <DivergenceLimit>1.e6</DivergenceLimit>
    <ArmijoUpdate>1</ArmijoUpdate>
    <ForcingStrategy>Constant</ForcingStrategy>
    <ConstantForcingTerm>1.e-4</ConstantForcingTerm>
  </NonlinearSolver>
  <LinearSolver>
    <MaximumIterations>5000</MaximumIterations>
    <AbsoluteTolerance>1.e-14</AbsoluteTolerance>
    <RelativeTolerance>1.e-14</RelativeTolerance>
    <DivergenceLimit>1.e6</DivergenceLimit>
    <BasisSize>50</BasisSize>
    <Preconditioning>1</Preconditioning>
  </LinearSolver>
  <ILUPP>
    <PreprocessingType>0</PreprocessingType>
    <PreconditionerNumber>1010</PreconditionerNumber>
    <MaxMultilevels>20</MaxMultilevels>
    <MemFactor>0.8</MemFactor>
    <PivotThreshold>1.75</PivotThreshold>
    <MinPivot>0.01</MinPivot>
  </ILUPP>
  <UseBoundaryDomainDescriptor>1</UseBoundaryDomainDescriptor>
  <!--- WARNING: Pressure filter might not be suitable for the current formulation of the problem -->
  <UsePressureFilter>0</UsePressureFilter>
</Param>
