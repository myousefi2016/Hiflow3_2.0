<Param>
  <Application>
    <Dimension>3</Dimension>
    <Platform>CPU</Platform>
    <Implementation>NAIVE</Implementation>
    <MatrixFormat>CSR</MatrixFormat>
    <Density>1.0</Density>
    <Viscosity>1.0e-3</Viscosity>
    <ChannelHeight>0.41</ChannelHeight>
    <MeanInflowSpeed>0.45</MeanInflowSpeed>
  </Application>
  <Mesh>
    <Filename>dfg_bench3d_rect.inp</Filename>
    <RefinementLevel>1</RefinementLevel>
  </Mesh>
  <Quadrature>
    <Name>GaussHexahedron</Name>
    <Points>27</Points>
    <CellType>Hexahedron</CellType>
  </Quadrature>
  <LinearSolver>
    <Name>GMRES</Name>
    <SizeBasis>50</SizeBasis>
    <Method>NoPreconditioning</Method>
    <MaxIterations>1000</MaxIterations>
    <AbsTolerance>1.0e-12</AbsTolerance>
    <RelTolerance>1.0e-12</RelTolerance>
    <DivTolerance>1.0e6</DivTolerance>
  </LinearSolver>
  <NonlinearSolver>
    <Name>Newton</Name>
    <MaxIterations>100</MaxIterations>
    <AbsTolerance>1.0e-9</AbsTolerance>
    <RelTolerance>1.0e-9</RelTolerance>
    <DivTolerance>1.0e6</DivTolerance>
    <Damping>None</Damping>
    <Forcing>None</Forcing>
  </NonlinearSolver>
  <Output>
    <MeshFilename>dfg_bench_local.pvtu</MeshFilename>
    <SolutionFilename>laminar_flow_values.pvtu</SolutionFilename>
    <InfoFilename>info_laminar_flow_benchmark.log</InfoFilename>
  </Output>
</Param>
